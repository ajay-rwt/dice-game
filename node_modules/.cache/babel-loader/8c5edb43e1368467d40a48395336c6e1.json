cA,IAAA,QAdA,sBAcA;AACA,WAAA,aAAA,GAAA,CAAA,KAAA,aAAA;AACA,WAAA,SAAA,GAAA,CAAA,KAAA,SAAA;AACA,WAAA,WAAA,GAAA,IAAA;AACA,KAlBA;AAmBA,IAAA,kBAnBA,8BAmBA,WAnBA,EAmBA;AACA,WAAA,WAAA,GAAA,WAAA;AACA,KArBA;AAsBA,IAAA,WAtBA,uBAsBA,iBAtBA,EAsBA,UAtBA,EAsBA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,YAAA,EAAA,iBAAA,EAAA,UAAA;;AACA,UAAA,iBAAA,IAAA,KAAA,WAAA,CAAA,SAAA,EAAA;AACA,QAAA,KAAA,CAAA,UAAA,GAAA,UAAA,CAAA;AACA,aAAA,QAAA;AACA,OAHA,MAGA;AACA,YAAA,UAAA,KAAA,KAAA,WAAA,CAAA,SAAA,CAAA,IAAA,EAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,WAAA;AACA,eAAA,iBAAA,GAAA,KAAA;AACA,eAAA,iBAAA,GAAA,IAAA;AACA,SAJA,MAIA;AACA,eAAA,iBAAA,GAAA,KAAA;AACA,eAAA,iBAAA,GAAA,IAAA;AACA;AACA;AACA;AArCA;AAfA,CAAA","sourcesContent":["<template>\n  <div id=\"app\">\n    <StartGame id=\"start-game\" v-if=\"!gameIsRunning\" @gameWasStarted=\"gameHasStarted($event)\"></StartGame>\n    <PlayerDetails\n      id=\"player-details\"\n      v-if=\"gameIsRunning && !inTheGame\"\n      @inTheGame=\"whenInTheGame\"\n      @gameDetailsProvided=\"setupPlayerDetails\"\n    ></PlayerDetails>\n    <div id=\"players\" v-if=\"gameIsRunning && inTheGame\">\n      <h2>WIN TARGET : {{ gameDetails.winTarget }}</h2>\n      <div id=\"sub-player\">\n        <Player\n          :isFirstPlayer=\"true\"\n          :isActive=\"isPlayerOneActive\"\n          :name=\"gameDetails.playerOne.name\"\n          :winTarget=\"gameDetails.winTarget\"\n          @turnGotOver=\"processGame\"\n        ></Player>\n        <Player\n          :isActive=\"isPlayerTwoActive\"\n          :name=\"gameDetails.playerTwo.name\"\n          :winTarget=\"gameDetails.winTarget\"\n          @turnGotOver=\"processGame\"\n        ></Player>\n      </div>\n      <br />\n      <br />\n      <button @click=\"quitGame\">Quit</button>\n    </div>\n  </div>\n</template>\n\n<script>\nimport StartGame from \"./components/StartGame.vue\";\nimport PlayerDetails from \"./components/PlayerDetails.vue\";\nimport Player from \"./components/Player.vue\";\n\nexport default {\n  components: {\n    StartGame,\n    PlayerDetails,\n    Player,\n  },\n  data: function () {\n    return {\n      gameIsRunning: false,\n      inTheGame: false,\n      gameDetails: {},\n      isPlayerOneActive: true,\n      isPlayerTwoActive: false,\n    };\n  },\n  methods: {\n    gameHasStarted(gameIsRunning) {\n      this.gameIsRunning = gameIsRunning;\n    },\n    whenInTheGame(inTheGame) {\n      this.inTheGame = inTheGame;\n      if (Math.floor(Math.random() * 2) + 1 === 1) {\n        this.isPlayerOneActive = true;\n        this.isPlayerTwoActive = false;\n      } else {\n        this.isPlayerOneActive = true;\n        this.isPlayerTwoActive = false;\n      }\n    },\n    quitGame() {\n      this.gameIsRunning = !this.gameIsRunning;\n      this.inTheGame = !this.inTheGame;\n      this.gameDetails = null;\n    },\n    setupPlayerDetails(gameDetails) {\n      this.gameDetails = gameDetails;\n    },\n    processGame(playersTotalScore, playerName) {\n      console.log(\"__________\", playersTotalScore, playerName);\n      if (playersTotalScore >= this.gameDetails.winTarget) {\n        alert(playerName + \" wins!!!\");\n        this.quitGame();\n      } else {\n        if (playerName === this.gameDetails.playerOne.name) {\n          console.log(\"heeere - \");\n          this.isPlayerOneActive = false;\n          this.isPlayerTwoActive = true;\n        } else {\n          this.isPlayerTwoActive = false;\n          this.isPlayerOneActive = true;\n        }\n      }\n    },\n  },\n};\n</script>\n\n<style scoped>\n#app {\n  height: 550px;\n  width: 70%;\n  margin: 100px auto 0 auto;\n  font-family: \"Dosis\", sans-serif;\n}\n\n#start-game {\n  background-color: burlywood;\n  width: 50%;\n  height: 50%;\n  margin: 100px auto 0 auto;\n  box-sizing: border-box;\n  padding: 10px;\n}\n\n#player-details {\n  background-color: burlywood;\n  width: 50%;\n  height: 50%;\n  margin: auto;\n  box-sizing: border-box;\n  padding: 10px;\n}\n\n#players {\n  box-sizing: border-box;\n  padding: 10px;\n}\n\n#sub-player {\n  display: flex;\n  box-sizing: border-box;\n  padding: 10px;\n  justify-content: space-between;\n}\n\n#players h2 {\n  text-align: center;\n}\n\n#players button {\n  display: block;\n  margin: auto;\n  box-sizing: border-box;\n  padding: 5px 10px 5px 10px;\n}\n</style>\n\n<style>\nbody {\n  background-color: whitesmoke;\n}\n</style>"],"sourceRoot":"src"}]}